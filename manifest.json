{
  "manifest_version": 3,
  "name": "Musescore Printable Viewer",
  "version": "1.0",
  "description": "Displays musescore.com sheets in a printer-friendly format",
  "author": "mangosaver (Cale)",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "https://musescore.com/*",
    "https://*.musescore.com/*"
  ]
}